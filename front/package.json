{
  "name": "nextjs-starter",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "build": "next build",
    "dev": "kill-port 3000 && NODE_OPTIONS='--inspect' next dev -p 3000",
    "prod": "next start",
    "type-check": "tsc --project tsconfig.json --noEmit --pretty",
    "lint": "eslint --quiet --fix --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "analyze": "cross-env ANALYZE=true npm run build",
    "check-all": "run-p type-check lint format",
    "api-codegen": "openapi --input ./swagger.json --output ./src/api --client axios --useOptions"
  },
  "dependencies": {
    "@sentry/nextjs": "7.7.0",
    "@tanstack/react-query": "4.6.0",
    "@tanstack/react-query-devtools": "4.6.0",
    "axios": "^0.27.2",
    "clsx": "^1.2.1",
    "form-data": "4.0.0",
    "lodash": "4.17.21",
    "mobx": "^6.6.1",
    "mobx-react-lite": "3.4.0",
    "nanoid": "^4.0.0",
    "next": "12.2.3",
    "next-seo": "5.5.0",
    "normalize.css": "^8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-use": "17.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@fullhuman/postcss-purgecss": "4.1.3",
    "@next/bundle-analyzer": "12.2.3",
    "@types/gtag.js": "^0.0.10",
    "@types/lodash": "4.14.182",
    "@types/node": "^18.6.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "autoprefixer": "10.4.7",
    "commitizen": "1.0.4",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "cssnano": "5.1.12",
    "cz-conventional-changelog": "3.0.1",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "express": "4.18.1",
    "http-proxy-middleware": "2.0.6",
    "husky": "8.0.1",
    "jest": "28.1.1",
    "kill-port": "^2.0.0",
    "lint-staged": "^13.0.1",
    "npm-run-all": "4.1.5",
    "openapi-typescript-codegen": "0.23.0",
    "postcss": "8.4.14",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "7.7.1",
    "prettier": "^2.7.1",
    "rimraf": "3.0.2",
    "sass": "^1.54.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">= 16",
    "npm": ">= 8"
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx}": [
      "npm run lint",
      "npm run format",
      "bash -c tsc --noEmit --pretty"
    ],
    "*.js": "eslint --cache --fix"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
